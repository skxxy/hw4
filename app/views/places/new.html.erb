<h1>New Place</h1>
<% if @current_user %>
  <%= form_with :model => @place do |form| %>
    <p>
      <%= form.label "name" %>
      <%= form.text_field "name" %>
    </p>
    <p>
      <%= form.submit %>
    </p>
  <% end %>
<%else%>
  You must first <a href="/login">Login</a>to add new places.
<% end %>
